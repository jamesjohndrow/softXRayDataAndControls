[psana]
#modules	= xppmodules.xpp_anaRad xppmodules.xpp_littleDat
modules	= xppmodules.xpp_littleDat

[xppmodules.xpp_anaRad]
# true -> use internal dark from same run, false: use official calibration (files???)
pedRun          = false
officialCal     = false
useMask         = true
#MaskEdge       = x y #x: number of edge pixels, y: mask middle pixels.
MaskEdge        = 5 1
#MaskUnbonded   = x y #x: mask unbonded, y: mask direct neighbors.
MaskUnbonded    = True False
commonMode      = false
gainAv          = 40
threshold       = 0
thresholdVal    = 0.1

#parameters for integration setup
xcen            = 93458.
ycen            = 93955.
pixel           = 100e-6
dis_to_sam      = 0.1
eBeam           = 9.
debug           = 0
keyStr          = ''

[xppmodules.xpp_littleDat]
# event selection: event codes
dirname = /reg/neh/home1/snelson/Workarea/ldat-ana-current/
cnffile = /reg/neh/home1/snelson/Workarea/ldat-ana-current/xppmodules/src/xpp_littleDat.cfg
xfel_off_code   = 162
laser_off_code  = 91
evrCodes = 162 150 164 140 141 142 143 144 145 146 40 41 42 43 44 45 46 90 91 92 93 94 95 96 97 98 190 191 192 193 194 
fileType=hdf5
writeData = true
#writeDataFree = false
debug = true
#timeIt = true

#additional EPICS variables
epicsPV = s1h_w
